<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iamicyconm.conm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.iamicyconm.conm.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.iamicyconm.conm.Bisection" android:windowSoftInputMode="stateHidden"/>
        <activity android:name="com.iamicyconm.conm.Jacobi" android:windowSoftInputMode="stateHidden" />
        <activity android:name="com.iamicyconm.conm.aboutapp"/>
        <activity android:name="com.iamicyconm.conm.Mainhome"/>
        <activity android:name="com.iamicyconm.conm.Seidel" android:windowSoftInputMode="stateHidden"/>
    </application>

</manifest>